        "\[IndentingNewLine]", 
        RowBox[{"nextRuleSet", "=", 
         RowBox[{"Rest", "[", "nextRuleSet", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"candidates", "=", 
         RowBox[{"StringJoin", "@@@", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"IntegerDigits", "[", 
             RowBox[{
              RowBox[{"ClueExpression", "[", "clueIdx", "]"}], "/.", 
