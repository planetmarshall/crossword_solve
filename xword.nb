           RowBox[{";;", ",", "1"}], "]"}], "]"}], ",", 
         RowBox[{"First", "[", "#", "]"}]}], "]"}], "&"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"connectedPatterns", "=", 
      RowBox[{"Select", "[", 
       RowBox[{"existingPatterns", ",", "connectedPatternIsUnsolved"}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "connectedPatterns", "]"}], " ", "\[NotEqual]",
         " ", "0"}], ",", "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
        "Replace", " ", "any", " ", "connected", " ", "patterns", " ", 
         "using", " ", "the", " ", "parent", " ", "pattern"}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"newPatternRules", "=", 
         RowBox[{"Cases", "[", 
