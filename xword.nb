Cell[BoxData[
 RowBox[{
  RowBox[{"GenerateNewPatterns", "[", 
   RowBox[{"existingPatterns_", ",", 
    RowBox[{"{", 
     RowBox[{"parentPattern_", ",", "clueIdx_"}], "}"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "connectedPatterns", ",", "newPatterns", ",", 
      "connectedPatternIsUnsolved", ",", "patternPairs", ",", 
      "newPatternRules"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"connectedPatternIsUnsolved", "=", 
      RowBox[{
       RowBox[{"MemberQ", "[", 
