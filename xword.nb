Cell[BoxData[
 RowBox[{
  RowBox[{"ExamineNextCandidate", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"solved_", ",", " ", "unsolved_"}], "}"}], ",", "rules_"}], 
   "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "nextRuleSet", ",", "clueIdx", ",", "cluePattern", ",", "nextRule", ",", 
      "candidates", ",", "nextPatterns", ",", "nextArgs", ",", "i", ",", 
      "solution"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"clueIdx", ",", "cluePattern"}], "}"}], " ", "=", " ", 
      RowBox[{"First", "[", "unsolved", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"nextRuleSet", "=", 
      RowBox[{"GenerateRules", "[", 
