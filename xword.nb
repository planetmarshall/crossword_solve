Cell[BoxData[
 RowBox[{
  RowBox[{"GenerateRules", "[", 
   RowBox[{"letters__", ",", "existingRules__"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"Any", " ", "new", " ", "symbols", " ", "that", " ", 
       RowBox[{"don", "'"}], "t", " ", "yet", " ", "have", " ", "rules"}], 
      " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"newLetters", "=", 
       RowBox[{"Complement", "[", 
        RowBox[{"letters", ",", 
         RowBox[{"First", "/@", "existingRules"}]}], "]"}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "Any", " ", "primes", " ", "not", " ", "yet", " ", "used", " ", "in", 
        " ", "a", " ", "rule"}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"primes", "=", 
       RowBox[{"Complement", "[", 
        RowBox[{
         RowBox[{"Array", "[", 
          RowBox[{"Prime", ",", "10"}], "]"}], ",", 
         RowBox[{"Last", "/@", "existingRules"}]}], "]"}]}], ",", 
      "\[IndentingNewLine]", "numLetters", ",", "numPerms", ",", "numPrimes", 
      ",", "newRules"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"numLetters", ",", "numPrimes"}], "}"}], "=", 
      RowBox[{"Length", "/@", 
       RowBox[{"{", 
        RowBox[{"newLetters", ",", "primes"}], "}"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"numPerms", "=", 
      RowBox[{
       RowBox[{"numPrimes", "!"}], "/", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"numPrimes", "-", "numLetters"}], ")"}], "!"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"newRules", "=", 
      RowBox[{"MapThread", "[", 
       RowBox[{"Rule", ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Table", "[", 
           RowBox[{"newLetters", ",", 
            RowBox[{"{", "numPerms", "}"}]}], "]"}], ",", 
          RowBox[{"Permutations", "[", 
           RowBox[{"primes", ",", 
            RowBox[{"{", "numLetters", "}"}]}], "]"}]}], "}"}], ",", "2"}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"Union", "[", 
        RowBox[{"existingRules", ",", "#"}], "]"}], "&"}], "/@", 
      "newRules"}]}]}], "]"}]}]], "Input",
